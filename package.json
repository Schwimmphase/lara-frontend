{
  "name": "lara-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@vueuse/core": "^9.12.0",
    "axios": "^1.2.3",
    "axios-mock-adapter": "^1.21.2",
    "class-transformer": "^0.5.1",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.28",
    "pinia-plugin-persistedstate": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.1"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.12"
  }
}
